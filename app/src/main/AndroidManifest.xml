<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nethanjan.projectuiux">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Learn Maori"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumberActivity"
            android:label="Numbers : Nga Tau  "/>
        <activity android:name=".FamilyActivity"
            android:label="Family Members : Whanau"/>
        <activity android:name=".ColorActivity"
            android:label="Colors : Nga Tae"></activity>
    </application>

</manifest>